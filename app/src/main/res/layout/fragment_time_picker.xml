<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                xmlns:app="http://schemas.android.com/apk/res-auto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:context="com.dslm.funddataanalysisapp.AddFundDialogFragment">
    <DatePicker
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/id_time_picker"/>
    <Button
            android:text="@string/cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_cancel"
            android:layout_below="@id/id_time_picker"
            android:layout_alignLeft="@id/id_time_picker"/>
    <Button
            android:text="@string/check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/date_check"
            android:layout_below="@id/id_time_picker"
            android:layout_alignRight="@id/id_time_picker"/>
</RelativeLayout>
